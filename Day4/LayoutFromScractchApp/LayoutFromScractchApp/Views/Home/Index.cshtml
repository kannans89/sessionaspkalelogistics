
<h1>Index</h1>

<button onclick="fnGetPartialView()">Get a PartialView</button>
<div id="timeStampDiv">
</div>
    <br/>
    <button onclick="fnLoadData()">LoadData(No Async)</button>
<button onclick="fnLoadDataASync()">LoadData( Async)</button>

    <img id="imgLoading"
         src="https://media2.giphy.com/media/3oEjI6SIIHBdRxXI40/giphy.gif?cid=ecf05e47seuqa5btc71x9ti62d84ov2q09fj2rnlr6mybgpa&rid=giphy.gif&ct=g" />






@section scriptContent {

    <script>

        $("#imgLoading").hide();

        function fnLoadDataASync() {

            $("#imgLoading").show();

            $.get("/home/GetDataNonBlocking", function (resp) {
                console.log(resp);
                // $("#timeStampDiv").html(resp);
                $("#imgLoading").hide();
            })

            console.log("end of fnLoadDataASync");
        
        }


        function fnLoadData() {
            $("#imgLoading").show();

            $.get("/home/GetDataBlocking", function (resp) {
                console.log(resp);
               // $("#timeStampDiv").html(resp);
                $("#imgLoading").hide();
            })
        
            console.log("end of fnLoadData");

        }





        
       // console.log($ == jQuery)
       // console.log($)
        function fnGetPartialView() {

            $.get("/home/GetPartialView", function (resp) {
                console.log(resp);
                $("#timeStampDiv").html(resp);
            })
           

            console.log("end of fnGetPartialView");
        }
    </script>
}